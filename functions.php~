<?php
function _Header() {
    echo "<HTML><HEAD><TITLE>Sample Page</TITLE></HEAD><BODY>";
    echo "<h4>Hello There</h4> ";
}

function _Footer() {
    echo "</BODY></HTML>";
}
function makeform(){
	echo "<form>";
}
function item($Text,$type,$name){
	echo "$Text:<input type='$type' name=$name><br>";
}
function endform(){
	echo "</form>";
}
?>

